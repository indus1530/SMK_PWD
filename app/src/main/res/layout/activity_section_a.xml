                android:id="@+id/fldGrpCVaa01"
                style="@style/cardView">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/bottom">

                        <TextView
                            android:id="@+id/qtxt_aa01"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@id/q_aa01"
                            android:text="@string/aa01" />

                        <TextView
                            android:id="@+id/q_aa01"
                            style="@style/quesNum"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignTop="@id/qtxt_aa01"
                            android:layout_alignBottom="@id/qtxt_aa01"
                            android:text="@string/Q_aa01" />

                    </RelativeLayout>

                    <io.blackbox_vision.datetimepickeredittext.view.DatePickerEditText
                        android:id="@+id/aa01"
                        style="@style/EditTextNum"
                        android:hint="dd-MM-yyyy"
                        android:gravity="center"
                        app:dateFormat="dd-MM-yyyy"
                        app:maxDate="CR_DATE"
                        app:minDate="CR_DATE"
                        app:theme="@style/DatePickerStyle" />

                </LinearLayout>

            </androidx.cardview.widget.CardView>


