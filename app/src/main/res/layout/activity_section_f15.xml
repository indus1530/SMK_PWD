            <androidx.cardview.widget.CardView
                android:id="@+id/fldGrpCVfo04"
                style="@style/cardView">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/fo04" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
