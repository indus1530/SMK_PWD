            android:orientation="horizontal">

            <TextView
                android:id="@+id/pEntries"
                style="@style/textviewh"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="Patient Entries" />

            <TextView
                android:id="@+id/countI"
                style="@style/textviewh"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="Count I: " />

        </LinearLayout>


