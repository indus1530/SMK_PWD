                        android:background="@drawable/bottom">

                        <TextView
                            android:id="@+id/qtxt_ib01"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@id/q_ib01"
                            android:text="@string/ib01" />

                        <TextView
                            android:id="@+id/q_ib01"
                            style="@style/quesNum"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignTop="@id/qtxt_ib01"
                            android:layout_alignBottom="@id/qtxt_ib01"
                            android:text="@string/Q_ib01" />

                    </RelativeLayout>

                    <RadioGroup
                        android:id="@+id/ib01"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <RadioButton
                            android:id="@+id/ib01a"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/ib01a" />

                        <RadioButton
                            android:id="@+id/ib01b"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/ib01b" />

                        <RadioButton
                            android:id="@+id/ib01c"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/ib01c" />

                    </RadioGroup>

                    <LinearLayout
                        android:id="@+id/llib01"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/colorAccent"
                                android:gravity="center"
                                android:text="@string/ib01d" />

                            <com.edittextpicker.aliazaz.EditTextPicker
                                android:id="@+id/ib01dx"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:hint="###"
                                android:inputType="number"
                                android:maxLength="3"
                                android:tag="ib01d"
                                app:maxValue="100"
                                app:minValue="0"
                                app:type="range" />

