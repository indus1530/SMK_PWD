                android:id="@+id/fldGrpCVib01"
                style="@style/cardView">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/bottom">

                        <TextView
                            android:id="@+id/qtxt_ib01"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@id/q_ib01"
                            android:text="@string/ib01" />

                        <TextView
                            android:id="@+id/q_ib01"
                            style="@style/quesNum"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignTop="@id/qtxt_ib01"
                            android:layout_alignBottom="@id/qtxt_ib01"
                            android:text="@string/Q_ib01" />
                    </RelativeLayout>

                    <RadioGroup
                        android:id="@+id/ib01"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <RadioButton
                            android:id="@+id/ib01a"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/ib01a" />

                        <RadioButton
                            android:id="@+id/ib01b"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/ib01b" />

                        <RadioButton
                            android:id="@+id/ib01c"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/ib01c" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:visibility='@{ib01a.checked ? View.GONE : View.VISIBLE}'>

                            <RadioButton
                                android:id="@+id/ib01d"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/ib01d" />

                            <com.edittextpicker.aliazaz.EditTextPicker
                                android:id="@+id/ib01dx"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="@string/ib01d"
                                android:inputType="number"
                                android:maxLength="3"
                                android:tag="ib01d"
                                android:text='@{ib01d.checked ? ib01dx.getText().toString() : ""}'

                                android:visibility='@{ib01d.checked? View.VISIBLE : View.GONE}'
                                app:maxValue="100"
                                app:type="range" />

                            <RadioButton
                                android:id="@+id/ib01e"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/ib01e" />

                            <com.edittextpicker.aliazaz.EditTextPicker
