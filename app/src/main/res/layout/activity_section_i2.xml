                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/bottom">

                        <TextView
                            android:id="@+id/qtxt_ib08"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@id/q_ib08"
                            android:text="@string/ib08" />

                        <TextView
                            android:id="@+id/q_ib08"
                            style="@style/quesNum"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignTop="@id/qtxt_ib08"
                            android:layout_alignBottom="@id/qtxt_ib08"
                            android:text="@string/Q_ib08" />
                    </RelativeLayout>

                    <RadioGroup
                        android:id="@+id/ib08"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <RadioButton
                            android:id="@+id/ib08a"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/ib08a" />

                        <RadioButton
                            android:id="@+id/ib08b"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/ib08b" />

                        <RadioButton
                            android:id="@+id/ib08c"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/ib08c" />

                        <RadioButton
                            android:id="@+id/ib08d"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/ib08d" />

                        <com.edittextpicker.aliazaz.EditTextPicker
                            android:id="@+id/ib08dx"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="@string/ib08d"
                            android:inputType="number"
                            android:maxLength="3"
                            android:tag="ib08d"
                            android:text='@{ib08d.checked ? ib08dx.getText().toString() : ""}'

                            android:visibility='@{ib08d.checked? View.VISIBLE : View.GONE}'
                            app:maxValue="100"
                            app:type="range" />

                        <RadioButton
                            android:id="@+id/ib08e"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/ib08e" />

                        <com.edittextpicker.aliazaz.EditTextPicker
                            android:id="@+id/ib08ex"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="@string/ib08e"
                            android:inputType="number"
                            android:maxLength="3"
                            android:tag="ib08e"
                            android:text='@{ib08e.checked ? ib08ex.getText().toString() : ""}'

                            android:visibility='@{ib08e.checked? View.VISIBLE : View.GONE}'
                            app:maxValue="100"
                            app:type="range" />
                    </RadioGroup>
                </LinearLayout>
            </androidx.cardview.widget.CardView><!--EndButton LinearLayout-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="end"
                android:layout_marginTop="24dp"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/btn_End"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="12dp"
                    android:background="@color/red_overlay"
                    android:onClick="@{() -> callback.BtnEnd()}"
                    android:text="Cancel"
                    android:textColor="@color/white" />

                <Button
                    android:id="@+id/btn_Continue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="12dp"
                    android:background="@color/green_overlay"
                    android:onClick="@{() -> callback.BtnContinue()}"
                    android:text="Save"
                    android:textColor="@color/white" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</layout>